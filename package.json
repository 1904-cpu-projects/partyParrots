    "express-session": "^1.16.2",
    "helmet": "^3.20.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.12.0",
    "pg": "^7.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",

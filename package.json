{
  "name": "grace-shopper-party-parrots",
  "version": "1.0.0",
  "description": "A fullstack e-commerce site.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "css:build": "node-sass --omit-source-map-url ./public/style/mystyles.scss ./public/style/style.css",
    "css:watch": "npm run css:build -- --watch",
    "test": "jest --config=jest.config.js -i",
    "test:dev": "jest --config=jest.config.js -i --watchAll",
    "start": "npm run server",
    "start:dev": "npm run css:watch & npm run build:dev & npm run server:dev",
    "build": "./node_modules/.bin/webpack  --config webpack.production.config.js",
    "build:dev": "webpack --watch",
    "server": "node ./server/index.js",
    "server:dev": "nodemon ./server/index.js --ignore public/ --ignore src/",
    "seed": "node seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1904-cpu-projects/team-jsn.git"
  },
  "keywords": [
    "react",
    "redux",
    "node",
    "express",
    "sequelize",
    "postgresql"
  ],
  "author": "Justin Stephanie Nick",
  "license": "ISC",
